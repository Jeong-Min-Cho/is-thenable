{
  "name": "is-thenable",
  "version": "1.0.0",
  "description": "Check if a value is thenable (has a then method) - Promises/A+ compliant",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "prepublishOnly": "npm run lint && npm run build && npm run test",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "thenable",
    "promise",
    "is-promise",
    "is-thenable",
    "check",
    "detect",
    "promise-like",
    "promises-aplus",
    "async",
    "then"
  ],
  "author": {
    "name": "Jeong Min Cho",
    "email": "jeongmincho4@gmail.com",
    "url": "https://www.npmjs.com/~jeongmincho"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jeong-Min-Cho/is-thenable.git"
  },
  "bugs": {
    "url": "https://github.com/Jeong-Min-Cho/is-thenable/issues"
  },
  "homepage": "https://github.com/Jeong-Min-Cho/is-thenable#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^8.57.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false
}
